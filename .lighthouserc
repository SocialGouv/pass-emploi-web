{
  "ci": {
      "collect": {
        "startServerCommand": "yarn start",
        "startServerReadyPattern": "Ready on http://localhost:3000",
        "startServerReadyTimeout": 600000,
        "url": [
          "http://localhost:3000/login"
        ],
          "numberOfRuns": 1,
          "settings": {
          "preset": "desktop"
        }
      },
      "assert": {
        "preset": "lighthouse:recommended"
      },
      "upload": {
        "target": "temporary-public-storage"
      },
      "server": {}
    }
}
